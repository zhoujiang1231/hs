<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hs.dao.UserDao">
    <resultMap id="UserList" type="User">
    </resultMap>
    <select id="getLoginUser" resultType="User" parameterType="User">
        SELECT * FROM user WHERE user_name=#{user_name} and user_password = #{user_password}
    </select>
    <select id="getAllStudent" resultMap="UserList">
        SELECT * FROM user where user_type=2
    </select>
</mapper>